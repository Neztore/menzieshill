  "description": "Client side for the Menzieshill Whitehall site (And the server which hosts it)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "admin:dev": "parcel watch src/admin/index.html --out-dir public/admin --public-url=/admin",
    "admin:build": "parcel build src/admin/index.html --public-url=/admin --out-dir public/admin --no-source-maps",
    "build": "concurrently \"npm run admin:build\" \"npm run render\"",
    "render": "node index.js",
    "watch": "node index.js watch",
    "dev": "concurrently \"npm run watch\" \"node dev-server.js\""
